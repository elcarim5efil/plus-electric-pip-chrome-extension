{
  "name": "wechat-sticker-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "base64-img": "^1.0.4",
    "image-to-base64": "^2.0.1",
    "moment": "^2.23.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "npm-watch build",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
